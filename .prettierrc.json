{
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 150,
    "tabWidth": 4,
    "useTabs": false,
    "quoteProps": "as-needed",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "requirePragma": false,
    "proseWrap": "preserve"
}
